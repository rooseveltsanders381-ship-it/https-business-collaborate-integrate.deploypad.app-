# Server Configuration
PORT=3000

# V5 Security
V5_AGENT_KEY=your_v5_security_token_here

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_supabase_anon_or_service_key_here

# Optional: Database Configuration
# DATABASE_URL=your_database_connection_string

# Node Environment
NODE_ENV=development
# Create the secure environment file
echo "V5_AGENT_KEY=YourSuperSecretKeyHere" > /srv/sanders/config/.env.v5

# Lock it down so only root can read it
chmod 600 /srv/sanders/config/.env.v5
chattr +i /srv/sanders/config/.env.v5
